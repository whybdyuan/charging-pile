<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yiyuan.menu.mapper.MenuMapper">
    <select id="getList" resultType="com.yiyuan.menu.domain.entity.Menu">
        SELECT
            menu_id,
            menu_name,
            parent_id,
            path,
            component,
            order_id,
            status,
            icon,
            create_by
        FROM sys_menu
        ORDER BY parent_id, order_id;
    </select>
</mapper>
